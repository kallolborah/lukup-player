<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/channellayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:orientation="vertical" >

<!--     android:background="#DD000000" -->
    
    <RelativeLayout
        android:id="@+id/bar"
        android:layout_width="match_parent"
        android:layout_height="124dip" >
        <LinearLayout
            android:id="@+id/bar1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal" >
			<RelativeLayout
                android:id="@+id/Chl"
                android:layout_width="207dip"
                android:layout_height="match_parent"
                android:layout_weight="1" >
                <RelativeLayout 
			      android:id="@+id/ChlRL"
			      android:background="@drawable/bg_channel_others"
			      android:layout_margin="8dip"
			      android:layout_width="match_parent"
				  android:layout_height="match_parent" >
					<RelativeLayout 
						android:id="@+id/chlbar"
						android:layout_centerInParent="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content" >
				        <TextView
							android:textColor="#ffffff"
							android:textSize="18dip"
							android:layout_gravity="center_vertical"
							android:singleLine="true"
							android:id="@+id/chlname"
							android:layout_centerVertical="true"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"/>
					</RelativeLayout>
				</RelativeLayout>
                <View
                    android:id="@+id/lineview0"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:background="#C0C0C0" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ProgRL1"
                android:layout_width="207dip"
                android:layout_height="match_parent"
                android:layout_weight="1" >
				<ImageView
                    android:id="@+id/icon1"
                    android:layout_width="96dip"
                    android:layout_height="96dip"
                    android:visibility="invisible"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="16dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/default_image" />

                <TextView
                    android:id="@+id/Progname1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="15dip"
                    android:layout_toRightOf="@+id/icon1"
                    android:ellipsize="end"
                    android:textColor="#ffffff"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/time1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Progname1"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="5dip"
                    android:layout_toRightOf="@+id/icon1"
                    android:ellipsize="end"
                    android:textColor="#ffffff"
                    android:textSize="13dip" />

                <View
                    android:id="@+id/lineview1"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:background="#C0C0C0" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ProgRL2"
                android:layout_width="207dip"
                android:layout_height="match_parent"
                android:layout_weight="1" >
				<ImageView
                    android:id="@+id/icon2"
                    android:layout_width="96dip"
                    android:layout_height="96dip"
                    android:visibility="invisible"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="16dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/default_image" />

                <TextView
                    android:id="@+id/Progname2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="15dip"
                    android:layout_toRightOf="@+id/icon2"
                    android:ellipsize="end"
                    android:textColor="#ffffff"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/time2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Progname2"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="5dip"
                    android:layout_toRightOf="@+id/icon2"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:textSize="13dip" />
               

                <View
                    android:id="@+id/lineview5"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:background="#C0C0C0" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ProgRL3"
                android:layout_width="207dip"
                android:layout_height="match_parent"
                android:layout_weight="1" >
				<ImageView
                    android:id="@+id/icon3"
                    android:layout_width="96dip"
                    android:layout_height="96dip"
                    android:visibility="invisible"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="16dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/default_image" />

                <TextView
                    android:id="@+id/Progname3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="15dip"
                    android:layout_toRightOf="@+id/icon3"
                    android:ellipsize="end"
                    android:textColor="#ffffff"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/time3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Progname3"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="5dip"
                    android:layout_toRightOf="@+id/icon3"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:textSize="13dip" />
               

                <View
                    android:id="@+id/lineview2"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:background="#C0C0C0" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ProgRL4"
                android:layout_width="207dip"
                android:layout_height="match_parent"
                android:layout_weight="1" >
				<ImageView
                    android:id="@+id/icon4"
                    android:layout_width="96dip"
                    android:layout_height="96dip"
                    android:visibility="invisible"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="16dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/default_image" />

                <TextView
                    android:id="@+id/Progname4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="15dip"
                    android:layout_toRightOf="@+id/icon4"
                    android:ellipsize="end"
                    android:textColor="#ffffff"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/time4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Progname4"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="5dip"
                    android:layout_toRightOf="@+id/icon4"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="#ffffff"
               		android:textSize="13dip" />

                <View
                    android:id="@+id/lineview3"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:background="#C0C0C0" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ProgRL5"
                android:layout_width="207dip"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_weight="1" >
				<ImageView
                    android:id="@+id/icon5"
                    android:layout_width="96dip"
                    android:layout_height="96dip"
                    android:visibility="invisible"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="16dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/default_image" />

                <TextView
                    android:id="@+id/Progname5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="15dip"
                    android:layout_toRightOf="@+id/icon5"
                    android:ellipsize="end"
                    android:textColor="#ffffff"
                    android:textSize="14dip" />

                <TextView
                    android:id="@+id/time5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Progname5"
                    android:layout_marginLeft="8dip"
                    android:layout_marginTop="5dip"
                    android:layout_toRightOf="@+id/icon5"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:textSize="13dip" />
               

                <View
                    android:id="@+id/lineview4"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:background="#C0C0C0" />
            </RelativeLayout>
        </LinearLayout>

        <View
            android:id="@+id/lineview"
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:layout_alignParentBottom="true"
            android:layout_weight="1"
            android:background="#C0C0C0" />
    </RelativeLayout>

</RelativeLayout>